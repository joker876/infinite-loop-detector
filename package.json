        "jasmine": "^4.1.0",
        "jasmine-ts-console-reporter": "^3.1.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^4.7.4",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0",
        "webpack-dev-server": "^4.11.1"
    },
